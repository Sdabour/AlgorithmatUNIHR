        function OnCheckAllChanged1(strCheckAllName, strCheckPrefix, strIDsName) {
            var objCheckAll = document.getElementById(strCheckAllName);
            var strCheckName;
            var objIDCol = [];
            var strIDs = document.getElementById(strIDsName).value;
            if (strIDs != null && strIDs != "") {
